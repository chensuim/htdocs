create table playerr(id char(28) not null primary key, roomr int(4), role char(1), voted boolean, resulted boolean, num char(1), msgid bigint(20));
create table roomr(roomrid int(4), totalnumber int(2), currentnumber int(2), mode int(1),roleflag char(10), turn int(1), status int(1), deny int(2), lancelot int(5), disagree int(2), last char(100), votes int(2),fail int(1), success int(1));
create table name(id char(28) not null, name char(21) not null, PRIMARY KEY(id,name));
